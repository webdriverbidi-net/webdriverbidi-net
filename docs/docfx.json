{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "WebDriverBiDi.dll"
          ],
          "src": "../src/WebDriverBiDi/bin/Release/netstandard2.0"
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "namespaceLayout": "flattened",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "title": "WebDriverBiDi.NET-Relaxed Documentation",
      "description": "Documentation for the WebDriverBiDi.NET-Relaxed library",
      "author": "WebDriverBiDi.NET-Relaxed Committers",
      "copyright": "Copyright (c) 2022-2025 WebDriverBiDi.NET-Relaxed Committers. All rights reserved.",
      "version": "${VERSION}",
      "_appLogoPath": "images/winston.png",
      "_appFaviconPath": "images/favicon.ico"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "templates/webdriverbidi"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
